---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Ogibero</title>
	<link href="https://ondiz.github.io/ogibero/errezetak/atom.xml" rel="self"/>
	<link href="https://ondiz.github.io/ogibero/errezetak/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://ondiz.github.io/ogibero/errezetak</id>
	<author>
		<name>Ondiz Zarraga</name>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://ondiz.github.io/ogibero{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://ondiz.github.io/ogibero/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
